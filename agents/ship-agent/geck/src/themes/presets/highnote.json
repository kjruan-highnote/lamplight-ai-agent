{
  "id": "highnote",
  "name": "Highnote",
  "description": "Modern, minimal theme inspired by Highnote's fintech design",
  "author": "GECK Team",
  "version": "1.0.0",
  "isDark": false,
  
  "colors": {
    "primary": "#00C55E",
    "primaryHover": "#00A84D",
    "primaryActive": "#008A3F",
    "primaryBorder": "#00C55E",
    "primaryBackground": "#E8F7ED",
    "primaryShadow": "rgba(0, 197, 94, 0.25)",
    
    "secondary": "#2C3E50",
    "secondaryHover": "#1A252F",
    "secondaryBorder": "#D1D5DB",
    "secondaryBackground": "#F3F4F6",
    
    "success": "#00C55E",
    "warning": "#F59E0B",
    "danger": "#EF4444",
    "info": "#3B82F6",
    
    "background": "#F9FAFB",
    "surface": "#FFFFFF",
    "surfaceHover": "#F3F4F6",
    "border": "#D1D5DB",
    "borderHover": "#9CA3AF",
    
    "text": "#111827",
    "textSecondary": "#4B5563",
    "textMuted": "#6B7280",
    "textInverse": "#FFFFFF",
    "placeholder": "#9CA3AF"
  },
  
  "typography": {
    "fontFamily": {
      "base": "-apple-system, BlinkMacSystemFont, 'Inter', 'Segoe UI', 'Helvetica Neue', sans-serif",
      "mono": "'SF Mono', 'Consolas', 'Monaco', 'Courier New', monospace",
      "display": "-apple-system, BlinkMacSystemFont, 'Inter', 'Segoe UI', 'Helvetica Neue', sans-serif"
    },
    "fontSize": {
      "xs": "0.75rem",
      "sm": "0.875rem",
      "base": "1rem",
      "lg": "1.125rem",
      "xl": "1.25rem",
      "2xl": "1.75rem",
      "3xl": "2.25rem"
    },
    "fontWeight": {
      "normal": 400,
      "medium": 500,
      "semibold": 600,
      "bold": 700
    }
  },
  
  "spacing": {
    "xs": "0.25rem",
    "sm": "0.5rem",
    "md": "1rem",
    "lg": "1.5rem",
    "xl": "2rem",
    "2xl": "3rem",
    "3xl": "4rem"
  },
  
  "borders": {
    "radius": {
      "none": "0",
      "sm": "0.375rem",
      "md": "0.5rem",
      "lg": "0.75rem",
      "full": "9999px"
    },
    "width": {
      "thin": "1px",
      "base": "1px",
      "thick": "2px"
    }
  },
  
  "effects": {
    "shadow": {
      "none": "none",
      "sm": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)",
      "md": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
      "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
      "xl": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04)",
      "glow": "0 0 0 3px rgba(0, 197, 94, 0.12)"
    },
    "transition": {
      "fast": "all 0.15s cubic-bezier(0.4, 0, 0.2, 1)",
      "base": "all 0.2s cubic-bezier(0.4, 0, 0.2, 1)",
      "slow": "all 0.3s cubic-bezier(0.4, 0, 0.2, 1)"
    },
    "blur": {
      "none": "0",
      "sm": "4px",
      "md": "8px",
      "lg": "16px"
    },
    "customEffects": {
      "gradient": "linear-gradient(135deg, #00C55E 0%, #00A84D 100%)",
      "subtleGradient": "linear-gradient(180deg, #FFFFFF 0%, #F9FAFB 100%)",
      "hoverLift": "translateY(-2px)",
      "cardBorder": "1px solid #D1D5DB"
    }
  }
}