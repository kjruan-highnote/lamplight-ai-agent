{
  "id": "highnote-dark",
  "name": "Highnote Dark",
  "description": "Dark mode version of Highnote theme with excellent contrast",
  "author": "GECK Team",
  "version": "1.0.0",
  "isDark": true,
  
  "colors": {
    "primary": "#00E06A",
    "primaryHover": "#00F574",
    "primaryActive": "#00C55E",
    "primaryBorder": "#00E06A",
    "primaryBackground": "#003D1F",
    "primaryShadow": "rgba(0, 224, 106, 0.3)",
    
    "secondary": "#94A3B8",
    "secondaryHover": "#CBD5E1",
    "secondaryBorder": "#475569",
    "secondaryBackground": "#1E293B",
    
    "success": "#00E06A",
    "warning": "#FBB516",
    "danger": "#F87171",
    "info": "#60A5FA",
    
    "background": "#0F172A",
    "surface": "#1E293B",
    "surfaceHover": "#334155",
    "border": "#334155",
    "borderHover": "#475569",
    
    "text": "#F1F5F9",
    "textSecondary": "#CBD5E1",
    "textMuted": "#94A3B8",
    "textInverse": "#0F172A",
    "placeholder": "#64748B"
  },
  
  "typography": {
    "fontFamily": {
      "base": "-apple-system, BlinkMacSystemFont, 'Inter', 'Segoe UI', 'Helvetica Neue', sans-serif",
      "mono": "'SF Mono', 'Consolas', 'Monaco', 'Courier New', monospace",
      "display": "-apple-system, BlinkMacSystemFont, 'Inter', 'Segoe UI', 'Helvetica Neue', sans-serif"
    },
    "fontSize": {
      "xs": "0.75rem",
      "sm": "0.875rem",
      "base": "1rem",
      "lg": "1.125rem",
      "xl": "1.25rem",
      "2xl": "1.75rem",
      "3xl": "2.25rem"
    },
    "fontWeight": {
      "normal": 400,
      "medium": 500,
      "semibold": 600,
      "bold": 700
    }
  },
  
  "spacing": {
    "xs": "0.25rem",
    "sm": "0.5rem",
    "md": "1rem",
    "lg": "1.5rem",
    "xl": "2rem",
    "2xl": "3rem",
    "3xl": "4rem"
  },
  
  "borders": {
    "radius": {
      "none": "0",
      "sm": "0.375rem",
      "md": "0.5rem",
      "lg": "0.75rem",
      "full": "9999px"
    },
    "width": {
      "thin": "1px",
      "base": "1px",
      "thick": "2px"
    }
  },
  
  "effects": {
    "shadow": {
      "none": "none",
      "sm": "0 1px 3px 0 rgba(0, 0, 0, 0.3), 0 1px 2px 0 rgba(0, 0, 0, 0.2)",
      "md": "0 4px 6px -1px rgba(0, 0, 0, 0.3), 0 2px 4px -1px rgba(0, 0, 0, 0.2)",
      "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.3), 0 4px 6px -2px rgba(0, 0, 0, 0.15)",
      "xl": "0 20px 25px -5px rgba(0, 0, 0, 0.3), 0 10px 10px -5px rgba(0, 0, 0, 0.14)",
      "glow": "0 0 0 3px rgba(0, 224, 106, 0.2)"
    },
    "transition": {
      "fast": "all 0.15s cubic-bezier(0.4, 0, 0.2, 1)",
      "base": "all 0.2s cubic-bezier(0.4, 0, 0.2, 1)",
      "slow": "all 0.3s cubic-bezier(0.4, 0, 0.2, 1)"
    },
    "blur": {
      "none": "0",
      "sm": "4px",
      "md": "8px",
      "lg": "16px"
    },
    "customEffects": {
      "gradient": "linear-gradient(135deg, #00E06A 0%, #00C55E 100%)",
      "subtleGradient": "linear-gradient(180deg, #1E293B 0%, #0F172A 100%)",
      "hoverLift": "translateY(-2px)",
      "cardBorder": "1px solid #334155"
    }
  }
}