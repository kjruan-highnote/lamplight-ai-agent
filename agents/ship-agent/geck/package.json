{
  "name": "geck",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@monaco-editor/react": "^4.7.0",
    "@netlify/functions": "^4.2.1",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^27.5.2",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-router-dom": "^5.3.3",
    "ajv": "^8.17.1",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.539.0",
    "mermaid": "^11.9.0",
    "monaco-editor": "^0.52.2",
    "mongodb": "^6.18.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.8.0",
    "react-scripts": "5.0.1",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "zustand": "^5.0.7"
  },
  "scripts": {
    "start": "node scripts/start-dev.js",
    "start:concurrent": "concurrently --kill-others -n \"APP,FUNC\" -c \"cyan,magenta\" \"npm run start:app\" \"npm run start:functions\"",
    "start:app": "craco start",
    "start:functions": "netlify functions:serve --port 9000",
    "start:generator": "cd .. && ./scripts/start-generator-service.sh",
    "start:all": "concurrently --kill-others -n \"APP,FUNC,GEN\" -c \"cyan,magenta,yellow\" \"npm run start:app\" \"npm run start:functions\" \"npm run start:generator\"",
    "start:netlify": "netlify dev",
    "build": "craco build && npm run build:functions",
    "build:functions": "netlify functions:build --src netlify/functions",
    "test": "craco test",
    "eject": "react-scripts eject",
    "logs:functions": "netlify functions:serve --port 9000 --debug",
    "import-programs": "ts-node scripts/import-programs.ts",
    "// Development Auth Modes //": "Various authentication modes for development",
    "dev": "cross-env REACT_APP_USE_DEV_AUTH=true npm start",
    "dev:engineer": "cross-env REACT_APP_USE_DEV_AUTH=true REACT_APP_DEV_AUTO_LOGIN=engineer npm start",
    "dev:solutions": "cross-env REACT_APP_USE_DEV_AUTH=true REACT_APP_DEV_AUTO_LOGIN=solutions npm start",
    "dev:admin": "cross-env REACT_APP_USE_DEV_AUTH=true REACT_APP_DEV_AUTO_LOGIN=admin npm start",
    "dev:noauth": "cross-env REACT_APP_USE_DEV_AUTH=true REACT_APP_SKIP_AUTH=true npm start",
    "dev:prod": "cross-env REACT_APP_USE_DEV_AUTH=false npm start",
    "// Development App Only Modes //": "Run only the React app with different auth modes",
    "app:dev": "cross-env REACT_APP_USE_DEV_AUTH=true npm run start:app",
    "app:engineer": "cross-env REACT_APP_USE_DEV_AUTH=true REACT_APP_DEV_AUTO_LOGIN=engineer npm run start:app",
    "app:solutions": "cross-env REACT_APP_USE_DEV_AUTH=true REACT_APP_DEV_AUTO_LOGIN=solutions npm run start:app",
    "app:admin": "cross-env REACT_APP_USE_DEV_AUTH=true REACT_APP_DEV_AUTO_LOGIN=admin npm run start:app",
    "// Quick Access //": "Shortcuts for common development scenarios",
    "quick": "npm run dev:engineer",
    "demo": "npm run dev:admin",
    "test:permissions": "npm run dev"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "netlify-cli": "^23.1.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2"
  }
}
